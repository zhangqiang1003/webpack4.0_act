{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {},
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", 2],
    "linebreak-style": ["off"],
    "quotes": ["error", "single"],
    "semi": 0,
    "no-unused-vars": ["warn"],
    "strict": 2
  }
}
